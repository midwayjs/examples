{
  "name": "demo-monorepo",
  "version": "1.0.0",
  "devDependencies": {
    "lerna": "^3.10.5"
  },
  "scripts": {
    "autod": "lerna run autod",
    "test": "lerna run test",
    "cov": "lerna run cov",
    "ci": "npm run build && npm run cov",
