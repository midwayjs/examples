{
  "name": "open-midway-examples",
  "version": "1.0.0",
  "devDependencies": {
    "git-hooks": "^1.1.10",
    "lerna": "^3.16.4",
    "lerna-relinker": "^1.4.0"
  },
  "scripts": {
    "autod": "lerna run autod",
    "test": "lerna run test",
    "cov": "lerna run cov",
    "ci": "npm run build && npm run cov",
